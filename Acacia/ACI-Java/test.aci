class Thing {}
let c = Thing();

class NewThing < Thing {}
let nc = NewThing();

class NewerThing < NewThing {}
let nwc = NewerThing();
                                  //should be:
println(instanceof(c, Thing));       //true
println(instanceof(c, NewThing));    //false
println(instanceof(nc, Thing));      //true
println(instanceof(nc, NewerThing)); //false
println(instanceof(nwc, Thing));     //true